{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parser": "@babel/eslint-parser",
  "plugins": ["babel", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn"
  }
}
